{
  "compilerOptions": {
    "lib": [
      "dom",
      "es6"
    ],
    "outDir": "dist",
    "module": "commonjs",
    "target": "es5",
    "strictNullChecks": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
    "vendor"
  ]
}
